{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "InfoSidebar": {
      "i18n": {
        "sourceLocale":"zh-Hant",
        "locales": {
          "zh-Hant": "src/assets/i18n/zh-Hant/messages.json",
          "en-Us":"src/assets/i18n/en-Us/messages.json",
          "zh-Hans":"src/assets/i18n/zh-Hans/messages.json"
        }
      },
      "projectType": "library",
      "root": "info-sidebar",
      "sourceRoot": "info-sidebar/src",
      "prefix": "his",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "info-sidebar/ng-package.json"
          },
          "configurations": {
            "zh-Hans":{
              "localize": ["zh-Hans"]
            },
            "zh-Hant":{
              "localize": ["zh-Hant"]
            },
            "en-Us":{
              "localize": ["en-Us"]
            },
            "production": {
              "tsConfig": "info-sidebar/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "info-sidebar/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "info-sidebar/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "info-sidebar/**/*.ts",
              "info-sidebar/**/*.html"
            ]
          }
        }
      }
    },
    "app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "standalone": true
        },
        "@schematics/angular:directive": {
          "standalone": true
        },
        "@schematics/angular:pipe": {
          "standalone": true
        }
      },
      "root": "app",
      "sourceRoot": "app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/app",
            "index": "app/src/index.html",
            "main": "app/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "app/src/favicon.ico",
              "app/src/assets"
            ],
            "styles": [
              "app/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "zh-Hans":{
              "localize": ["zh-Hans"]
            },
            "zh-Hant":{
              "localize": ["zh-Hant"]
            },
            "en-Us":{
              "localize": ["en-Us"]
            },
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "app:build:production"
            },
            "development": {
              "browserTarget": "app:build:development"
            },
            "zh-Hans":{
              "browserTarget": "app:build:zh-Hans"
            },
            "zh-Hant":{
              "browserTarget": "app:build:zh-Hant"
            },
            "en-Us":{
              "browserTarget": "app:build:en-Us"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app:build"
          },
          "configurations": {
            "zh-Hant":{
              "format":"json",
              "outputPath": "src/assets/i18n/zh-Hant"
            },
            "zh-Hans":{
              "format":"json",
              "outputPath": "src/assets/i18n/zh-Hans"
            },
            "en-Us":{
              "format":"json",
              "outputPath": "src/assets/i18n/en-Us"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "app/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "app/src/favicon.ico",
              "app/src/assets"
            ],
            "styles": [
              "app/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
